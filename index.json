{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 1000,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "css"
}
